{
  "name": "line-liff",
  "author": "Puchida Co., Ltd.",
  "private": true,
  "scripts": {
    "dev": "vite --port 4000",
    "tunnel": "~/ngrok http --region=ap 4000",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint --ext js,ts,tsx --cache --fix src/"
